.so ../man_macros/suds_man_macros
.TH LOCTRACE 5 "6 February 1994" "pc_suds" "PC_SUDS STRUCTURE"
.SH NAME
loctrace \- location of trace
.SH SYNOPSIS
.nf
.ta 0.7i +1.4i +0.7i +0.7i +0.7i +0.7i +0.7i +0.7i +0.7i
.ft B
typedef	struct {
	FIXED	structure_type;
	FIXED	structure_len;
	FIXED	len_netn_G;
	STRING	network[4];
	FIXED	len_st_nam_G;
	STRING	st_name[5];
	CODE1	component;
	CODE2	inst_type;
	CHRPTR	fileloc;
	CHRPTR	tapeloc;
	INT4	beginloc;
} PCSUDS_LOCTRACE;
.sp 0.5
#define PC_LOCTRACE	8L
.fi
.ft R
.SH DESCRIPTION
Not commonly used in PC-SUDS.
.SH MEMBERS
.TP
\fBstructure_type\fI structure type\fR
Define number of this type of structure.
.TP
\fBstructure_len\fI structure length\fR
Length of this structure in bytes.
.TP
\fBlen_netn_G\fI len net name\fR
.TP
\fBnetwork\fI network name\fR
.TP
\fBlen_st_nam_G\fI len stat name\fR
.TP
\fBst_name\fI station name\fR
.BB
index_string=true
.EB
.TP
\fBcomponent\fI component\fR
.BB
codelist=comps
.EB
.TP
\fBinst_type\fI instrument type\fR
.BB
codelist=inst_type
.EB
.TP
\fBfileloc\fI file name\fR
Pointer to pathname in file system.
.TP
\fBtapeloc\fI tape name\fR
Pointer to name of tape or offline storage.
.TP
\fBbeginloc\fI offset in file\fR
Bytes from begining of file to trace.
.SH "SEE ALSO"
.SH AUTHOR
Robert Banfill, January 1991
