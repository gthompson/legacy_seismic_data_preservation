.so ../man_macros/suds_man_macros
.TH EVDESCR 5 "6 February 1994" "pc_suds" "PC_SUDS STRUCTURE"
.SH NAME
evdescr \- descriptive information about an event
.SH SYNOPSIS
.nf
.ta 0.7i +1.4i +0.7i +0.7i +0.7i +0.7i +0.7i +0.7i +0.7i
.ft B
typedef	struct {
	FIXED	structure_type;
	FIXED	structure_len;
	FIXED	len_eqname;
	STRING	eqname[20];
	FIXED	len_country;
	STRING	country[16];
	FIXED	len_state;
	STRING	state[16];
	INT2	localtime;
	INT2	spareB;
	PAD4	pad_T;
} PCSUDS_EVDESCR;
.sp 0.5
#define PC_EVDESCRIPT	13L
.fi
.ft R
.SH DESCRIPTION
Not commonly used in PC-SUDS.  Name and location of an event.
.SH MEMBERS
.TP
\fBstructure_type\fI structure type\fR
Define number of this type of structure.
.TP
\fBstructure_len\fI structure length\fR
Length of this structure in bytes.
.TP
\fBlen_eqname\fI len eq name\fR
.TP
\fBeqname\fI earthquake name\fR
Popular name used to refer to this earthquake.
.BB
index_string=true
.EB
.TP
\fBlen_country\fI len county\fR
.TP
\fBcountry\fI county\fR
Country of earthquake.
.TP
\fBlen_state\fI len state\fR
.TP
\fBstate\fI state\fR
State, province or other political subdivision.
.TP
\fBlocaltime\fI local time diff\fR
Hours to add to GMT to get local time.
.TP
\fBspareB\fI for future use\fR
.TP
\fBpad_T\fI padding\fR
.SH "SEE ALSO"
